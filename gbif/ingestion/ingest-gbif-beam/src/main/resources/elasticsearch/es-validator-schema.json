{
    "dynamic_templates": [
      {
        "verbatim_string": {
          "path_match": "verbatim.*",
          "match_mapping_type": "string",
          "mapping": {  "type": "keyword" }
        }
      },
      {
        "verbatim_boolean": {
          "path_match": "verbatim.*",
          "match_mapping_type": "boolean",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "verbatim_date": {
          "path_match": "verbatim.*",
          "match_mapping_type": "date",
          "mapping": { "type": "keyword" }
        }
