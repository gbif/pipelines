      {
        "verbatim_double": {
          "path_match": "verbatim.*",
          "match_mapping_type": "double",
          "mapping": { "type": "keyword" }
        }
      },
      {
        "verbatim_long": {
          "path_match": "verbatim.*",
          "match_mapping_type": "long",
          "mapping": { "type": "keyword" }
        }
      }
